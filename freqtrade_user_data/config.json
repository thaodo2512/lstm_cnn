{
  "max_open_trades": 1,
  "stake_currency": "USDT",
  "stake_amount": "unlimited",
  "fiat_display_currency": "USD",
  "trading_mode": "futures",
  "margin_mode": "isolated",
  "timeframe": "1h",
  "dry_run": true,
  "strategy": "FreqAIHybridExample",
  "freqaimodel": "HybridTimeseriesFreqAIModel_tinhn",
  "exchange": {
    "name": "binance",
    "key": "",
    "secret": "",
    "ccxt_config": { "enableRateLimit": true },
    "ccxt_async_config": { "enableRateLimit": true },
    "pair_whitelist": ["BTC/USDT"],
    "pair_blacklist": []
  },
  "pairlists": [
    { "method": "StaticPairList" }
  ],
  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080,
    "verbosity": "info",
    "jwt_secret_key": "change-me",
    "username": "admin",
    "password": "changeme"
  },
  "entry_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0
  },
  "freqai": {
    "enabled": true,
    "model_classname": "HybridTimeseriesFreqAIModel_tinhn",
    "identifier": "BTC_fast",
    "train_period_days": 30,
    "backtest_period_days": 7,
    "feature_parameters": {
      "include_timeframes": ["1h"],
      "include_corr_pairlist": [],
      "label_period_candles": 24,
      "include_shifted_candles": 0,
      "indicator_periods_candles": [14]
    },
    "data_split_parameters": {
      "test_size": 0.25,
      "shuffle": false
    },
    "purge_old_models": 2,
    "model_training_parameters": {
      "window_size": 48,
      "horizon": 1,
      "stride": 2,
      "batch_size": 128,
      "epochs": 10,
      "lr": 0.001,
      "grad_clip": 1.0,
      "early_stopping_patience": 4,
      "use_minmax": false,
      "num_workers": 4,
      "variable_lengths": false,
      "min_seq_len": 20,
      "pooling": "last",
      "classification": false,
      "capture_attention": false,
      "bidirectional": false,
      "lstm_hidden": 96,
      "lstm_layers": 1,
      "lstm_dropout": 0.1,
      "d_model": 96,
      "nhead": 2,
      "num_transformer_layers": 1,
      "ff_dim": 256,
      "transformer_dropout": 0.1,
      "compile_model": true,
      "n_jobs": 4
    }
  }
}
