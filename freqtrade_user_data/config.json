{
  "max_open_trades": 1,
  "stake_currency": "USDT",
  "stake_amount": "unlimited",
  "fiat_display_currency": "USD",
  "timeframe": "1h",
  "dry_run": true,
  "strategy": "FreqAIHybridExample",
  "pairlists": [
    { "method": "StaticPairList", "pairs": ["BTC/USDT"] }
  ],
  "exchange": {
    "name": "binance",
