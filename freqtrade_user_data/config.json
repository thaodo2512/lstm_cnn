{
  "max_open_trades": 2,
  "stake_currency": "USDT",
  "stake_amount": "unlimited",
  "fiat_display_currency": "USD",
  "trading_mode": "futures",
  "margin_mode": "isolated",
  "timeframe": "5m",
  "dry_run": true,
  "strategy": "ichiV1",
  "freqaimodel": "HybridTimeseriesFreqAIModel_tinhn",
  "exchange": {
    "name": "binance",
    "key": "",
    "secret": "",
    "ccxt_config": {
      "enableRateLimit": true,
      "options": {
        "defaultType": "future"
      }
    },
    "ccxt_async_config": {
      "enableRateLimit": true,
      "options": {
        "defaultType": "future"
      }
    },
    "pair_whitelist": [
      "BTC/USDT:USDT",
      "ETH/USDT:USDT"
    ],
    "pair_blacklist": []
  },
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080,
    "verbosity": "info",
    "jwt_secret_key": "change-me",
    "username": "admin",
    "password": "changeme"
  },
  "telegram": {
    "enabled": false,
    "token": "",
    "chat_id": ""
  },
  "entry_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0
  },
  
  "tradable_balance_ratio": 0.3,
  
  "freqai": {
    "enabled": true,
    "model_classname": "HybridTimeseriesFreqAIModel_tinhn",
    "identifier": "BTC_fast",
    "train_period_days": 90,
    "backtest_period_days": 30,
    "live_retrain_hours": 24,
    "feature_parameters": {
      "include_timeframes": [
        "5m",
        "1h"
      ],
      "include_corr_pairlist": [],
      "label_period_candles": 24,
      "include_shifted_candles": 0,
      "indicator_periods_candles": [
        14
      ]
    },
    "data_split_parameters": {
      "test_size": 0.25,
      "shuffle": false
    },
    "purge_old_models": 2,
    "model_training_parameters": {
      "window_size": 96,
      "horizon": 1,
      "stride": 1,
      "batch_size": 512,
      "epochs": 15,
      "lr": 0.001,
      "grad_clip": 1.0,
      "early_stopping_patience": 4,
      "use_minmax": false,
      "num_workers": 4,
      "variable_lengths": false,
      "min_seq_len": 20,
      "pooling": "last",
      "classification": false,
      "capture_attention": false,
      "bidirectional": false,
      "lstm_hidden": 192,
      "lstm_layers": 4,
      "lstm_dropout": 0.1,
      "d_model": 192,
      "nhead": 4,
      "num_transformer_layers": 4,
      "ff_dim": 768,
      "transformer_dropout": 0.1,
      "compile_model": true,
      "n_jobs": 4
    }
  }
}
